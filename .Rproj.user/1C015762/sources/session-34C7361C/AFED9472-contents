data.frame(
  draft_year = c(2018:2024),
  draft_date = c(
    '2018-04-26',
    '2019-04-25',
    '2020-04-23',
    '2021-04-29',
    '2022-04-28',
    '2023-04-27',
    '2024-04-25'
  )
) %>%
  mutate(draft_date = as.Date(draft_date, format = '%Y-%m-%d')) -> date_xwalk

remove_paren <- function(x, string = "") {
  gsub(paste0("\\s*\\(", string, "[^\\)]+\\)"), "", x)
}

remove_end_paren <- function(x){
  sub(" \\(", ", ", x) %>%
    sub(")", "", .)
}

remove_semicolon <- function(x) {
  gsub("[;]", ",", x)
}

remove_emdash <- function(x) {
  gsub(" \\p{Pd}", ",", x, perl = TRUE)
}

remove_colon <- function(x) {
  gsub("[:]", ",", x)
}

remove_period <- function(x) {
  sub("[.]", ",", x)
}

remove_lowup <- function(x) {
  gsub("(?<=([a-z]))(?=[A-Z]|[0-9]|\\()", ", ", x, perl = TRUE)
}

detect_str <- function(str, patt = "[0-9][.] ") {
  str[grepl(patt, str)]
}

detect_chr <- function(str, n = 150) {
  str[nchar(str) <= n]
}

df_plus_name <- function(x, nm = 'draft') {
  x %>%
    as.data.frame %>%
    setNames(nm)
}

getText <- function(x) {
  gettxt(x, 'ASCII') %>%
    strsplit("\n") %>%
    unlist %>%
    str_squish %>%
    .[. != ''] %>%
    unique %>%
    iconv("latin1", "ASCII", sub = "") %>%
    sub("  ", ", ", .) %>%
    str_squish %>%
    gsub("’", "'", .)
}

split_picks <- function(x) {
  paste(x, collapse = ', ') %>%
    str_split("[0-9][0-9][0-9][.] ") %>%
    unlist %>%
    str_split("[0-9][0-9][.] ") %>%
    unlist %>%
    str_split("[0-9][.] ") %>%
    unlist %>%
    .[. != '']
}

df_separate <- function(x,
                        nm = 'draft',
                        sp = ', ',
                        it = c('pick',
                               'team',
                               'name',
                               'position',
                               'school')) {
  x %>%
    as.data.frame %>%
    setNames(nm) -> tmp
  
  separate(
    data = tmp,
    col = nm,
    sep = sp,
    into = it
  )
}

detect_nbr <- function(str) {
  str[substring(str, 0, 1) %in% 1:9]
}

